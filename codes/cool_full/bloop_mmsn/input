&params	smallq = -0.42857143	!temperature gradient
	smallp = -2.78571      	!midplane density gradient
	dist   =  20.0   	!distance in AU  
	gmma   =  1.4     	!ratio of specific heats
        bgmma  =  1.011     	!structural index  
	   zmax=  10.0          !height in terms of isothermal scale heights
	    vbc= 'nolagp'   	!vertical bc (nolagp, nozvel)
        gterms =.false.   / 	!extra terms in x-mom and energy eqn due to radial entrop gradient (still NO radial density gradient) 
&loop     kxmin= 20.0       	!minimum value of kx*Hiso 
          kxmax= 100         	!maximum value of kx*Hiso
          nkx =  51             !sample number
        bmin  =  0.001          !obsolete 
        bmax  =  0.100          !obsolete 
          nb  =  1              !only do one case 
   eigen_trial=.false.          !mode selection based on previous list of eigenvalues?
   eigenv_out =.true.          !output eigenvectors?
  eigen_refine=.false.     /    !are we updating solutions to higher resolution? if so, nb needs to match num. of lines in 'baxis.dat'
&grid       nz =257       /     !vertical grid points
